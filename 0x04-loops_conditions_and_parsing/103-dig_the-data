#!/usr/bin/env bash
# This script groups visitors by IP and HTTP status code from an Apache log file,
# and displays the data ordered by the number of occurrences.

# Assuming apache-access.log is in the current directory or provide the full path.
awk '{count[$1" "$9]++} END {for (combo in count) print count[combo], combo}' apache-access.log | sort -rn
